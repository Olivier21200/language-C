#include <stdio.h>
#include <stdlib.h>
#include <time.h>
// Programme de tri à bulles
int main()
{
    const int N = 30;
    int TAB[N];
    int i, j, temp;
    int permutation = 1;

    // initialisation du générateur de nombres aléatoires
    srand(time(NULL));

    // remplissage du tableau avec des valeurs aléatoires entre 0 et 50
    {
        TAB[i] = rand() % 51;
        printf("%d ", TAB[i]);
    }
    printf("\n");
    return 0;
}